{
  "name": "client-portal",
  "version": "2.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepush": "run-p lint format type-check",
    "update-locals": "node --env-file=.env.development update-locals.mjs"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.4.1",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@medrunner-services/api-client": "^0.18.1",
    "apexcharts": "^3.46.0",
    "discord-markdown": "^2.5.1",
    "dompurify": "^3.0.9",
    "flowbite": "^2.3.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.19",
    "vue-i18n": "^9.9.1",
    "vue-router": "^4.3.0",
    "vue3-apexcharts": "^1.4.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node18": "^18.2.2",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-vue": "^9.22.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27",
    "yauzl": "^3.1.0"
  }
}
