{
  "name": "client-portal",
  "version": "2.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepush": "run-p lint format type-check",
    "update-locals": "node update-locals.mjs"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.3.3",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@medrunner-services/api-client": "^0.16.0",
    "apexcharts": "^3.44.0",
    "discord-markdown": "^2.5.1",
    "flowbite": "^2.1.1",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "vue3-apexcharts": "^1.4.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19",
    "yauzl": "^2.10.0"
  }
}
