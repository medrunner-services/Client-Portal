{
  "name": "client-portal",
  "version": "2.6.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepush": "run-p lint format type-check",
    "update-locals": "node --env-file=.env.development update-locals.js",
    "update-packages": "ncu -i -u --format group"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^5.3.1",
    "@medrunner/api-client": "^0.1.4",
    "apexcharts": "^4.0.0",
    "discord-markdown": "^2.5.1",
    "dompurify": "^3.1.7",
    "flowbite": "^2.5.2",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5",
    "vue3-apexcharts": "^1.7.0"
  },
  "devDependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@tailwindcss/forms": "^0.5.9",
    "@tsconfig/node18": "^18.2.4",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.6.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^9.14.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.48",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.10",
    "yauzl": "^3.2.0"
  }
}
