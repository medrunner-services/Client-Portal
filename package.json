{
  "name": "client-portal",
  "version": "2.5.8",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepush": "run-p lint format type-check",
    "update-locals": "node --env-file=.env.development update-locals.js",
	"update-packages": "ncu -i -u --format group"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@medrunner/api-client": "^0.1.1",
    "@volar/language-server": "2.4.0-alpha.18",
    "@vue/language-server": "2.0.28",
    "apexcharts": "^3.49.1",
    "discord-markdown": "^2.5.1",
    "dompurify": "^3.1.5",
    "flowbite": "^2.3.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.30",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0",
    "vue3-apexcharts": "^1.5.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node18": "^18.2.4",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.22",
    "yauzl": "^3.1.3"
  }
}
