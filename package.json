{
  "name": "client-portal",
  "version": "2.7.2",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepush": "run-p lint format type-check",
    "update-locals": "node --env-file=.env.development update-locals.js",
    "update-packages": "ncu -i -u --format group"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@medrunner/api-client": "0.2.7",
    "apexcharts": "^4.4.0",
    "dompurify": "^3.2.4",
    "flowbite": "^2.5.2",
    "markdown-it": "^14.1.0",
    "pinia": "^2.3.1",
    "posthog-js": "^1.215.6",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.1",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tsconfig/node18": "^18.2.4",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.1",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite": "^6.1.0",
    "vue-tsc": "^2.2.0",
    "yauzl": "^3.2.0"
  }
}
