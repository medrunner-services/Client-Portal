{
  "name": "client-portal",
  "version": "2.6.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepush": "run-p lint format type-check",
    "update-locals": "node --env-file=.env.development update-locals.js",
    "update-packages": "ncu -i -u --format group"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.0",
    "@medrunner/api-client": "0.1.4",
    "apexcharts": "^4.1.0",
    "discord-markdown": "^2.5.1",
    "dompurify": "^3.2.1",
    "flowbite": "^2.5.2",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0"
  },
  "devDependencies": {
    "@microsoft/signalr": "^8.0.7",
    "@tailwindcss/forms": "^0.5.9",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^22.10.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.6.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^9.15.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.31.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.15",
    "typescript": "5.6.3",
    "vite": "^6.0.0",
    "vue-tsc": "^2.0.29",
    "yauzl": "^3.2.0"
  }
}
