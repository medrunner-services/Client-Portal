{
  "name": "client-portal",
  "version": "1.2.10",
  "private": true,
  "type": "module",
  "web-types": "./web-types.json",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check build-only",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "prepush": "run-p prettier:fix lint:fix type-check",
    "update-locals": "node update-locals.mjs"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.1.2",
    "@formkit/auto-animate": "^0.7.0",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@medrunner-services/api-client": "^0.14.3",
    "axios": "^1.2.2",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-i18n": "9.3.0-beta.24",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.4",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.1.4",
    "eslint": "^8.44.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "npm-run-all": "^4.1.2",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vue-tsc": "^1.8.4",
    "yauzl": "^2.10.0"
  }
}
