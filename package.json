{
  "name": "client-portal",
  "version": "2.5.8",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-network": "vite --host --mode development-network",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:staging": "run-p type-check build-only:staging",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepush": "run-p lint format type-check",
    "update-locals": "node --env-file=.env.development update-locals.js"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@medrunner/api-client": "0.1.3",
    "@microsoft/signalr": "^8.0.7",
    "apexcharts": "^3.54.0",
    "discord-markdown": "^2.5.1",
    "dompurify": "^3.1.7",
    "flowbite": "^2.5.2",
    "pinia": "^2.2.4",
    "vue": "^3.5.11",
    "vue-i18n": "^10.0.3",
    "vue-router": "^4.4.5",
    "vue3-apexcharts": "^1.6.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tailwindcss/forms": "^0.5.9",
    "@tsconfig/node18": "^18.2.4",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6",
    "yauzl": "^3.1.3"
  }
}
