    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.2",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
